hi clear
if exists("syntax_on")
	syntax reset
endif

let g:colors_name = "trym"
set background=dark

hi Normal ctermfg=15

hi LineNr ctermfg=11
hi Visual ctermbg=8

hi MatchParen ctermfg=6
hi Comment ctermfg=14
hi Constant ctermfg=13
hi Special ctermfg=224
hi Identifier ctermfg=14 cterm=bold
hi Statement ctermfg=11
hi PreProc ctermfg=81
hi Type ctermfg=121
hi Underline ctermfg=81 cterm=underline
